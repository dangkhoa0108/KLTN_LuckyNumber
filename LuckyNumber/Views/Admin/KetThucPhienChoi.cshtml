    <script src="~/assets1/js/components/asscrollable.js"></script>
    <script src="~/assets1/js/components/animsition.js"></script>
    <script src="~/assets1/js/components/slidepanel.js"></script>
    <script src="~/assets1/js/components/switchery.js"></script>
